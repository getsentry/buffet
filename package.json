{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "concurrently \"npm:dev:*\"",
    "dev:nuxt": "nuxi dev",
    "dev:supabase": "supabase start",
    "supabase:typegen": "supabase gen types typescript --local > supabase/types.gen.ts",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint:js": "eslint --ext \".ts,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "pnpm lint:js && pnpm lint:prettier",
    "lintfix": "prettier --write --list-different . && pnpm lint:js --fix"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@nuxtjs/supabase": "^1.1.3",
    "@nuxtjs/tailwindcss": "^6.8.1",
    "@vueuse/nuxt": "^10.5.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "nuxt": "^3.8.0",
    "prettier": "3.0.3",
    "supabase": "^1.106.1",
    "typescript": "^5.2.2",
    "vue": "^3.3.7",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.22"
  },
  "dependencies": {
    "@ark-ui/vue": "^0.8.0",
    "@supabase/supabase-js": "^2.38.4",
    "@vee-validate/nuxt": "^4.11.8",
    "h3-zod": "^0.5.2",
    "zod": "^3.22.4"
  }
}
