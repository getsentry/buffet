{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "concurrently \"npm:dev:*\"",
    "dev:nuxt": "nuxi dev --inspect",
    "dev:supabase": "supabase start",
    "migrate:local": "supabase migration up --include-all --local",
    "supabase:typegen": "supabase gen types typescript --local > supabase/types.gen.ts",
    "supabase:migrate": "supabase db diff | supabase migration new",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint:js": "eslint --ext \".ts,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "pnpm lint:js && pnpm lint:prettier",
    "lintfix": "prettier --write --list-different . && pnpm lint:js --fix"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@nuxtjs/supabase": "^1.1.3",
    "@nuxtjs/tailwindcss": "^6.8.1",
    "@types/uuid": "^9.0.7",
    "@vueuse/nuxt": "^10.7.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "nuxt": "^3.8.0",
    "prettier": "3.1.1",
    "supabase": "^1.123.4",
    "typescript": "^5.3.3",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.25"
  },
  "dependencies": {
    "@sentry/cli": "^2.23.0",
    "@sentry/node": "^7.88.0",
    "@sentry/profiling-node": "^1.3.2",
    "@sentry/vue": "^7.88.0",
    "@supabase/supabase-js": "^2.39.0",
    "@vee-validate/nuxt": "^4.12.2",
    "@vee-validate/zod": "^4.12.2",
    "h3-zod": "^0.5.2",
    "url-metadata": "^3.3.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  }
}
